<main>
    <div class="card">
        <div class="card-body">
            {{#with heading}}
            {{!-- BLOCK 1 --}}
            <div class="card">
                <div class="card-body">
                    {{#with main}}
                    <h1>{{title}}</h1>
                    <p>{{text}}</p>
                    <div>
                        {{#with button_text}}
                        <p><button class="btn btn-primary">{{solid}}</button></p>
                        <br>

                        <button class="btn btn-outline-primary">{{outline}}</button>
                        {{/with}}
                    </div>
                    {{/with}}
                </div>
            </div>
            <br>
            {{!-- BLOCK 2 --}}
            <div class="card">
                <div class="card-body">
                    {{#with second}}
                    <h2>{{title}}</h2>
                    <p>{{text}}</p>
                    <div>
                        {{#with button_text}}
                        <p><button class="btn btn-secondary">{{solid}}</button></p>

                        <button class="btn btn-outline-secondary">{{outline}}</button>
                        {{/with}}
                    </div>
                    {{/with}}
                </div>
            </div>
            <br>
            {{!-- BLOCK 3 --}}
            <div class="card">
                <div class="card-body">
                    {{#with subblock}}
                    <h3>{{title}}</h3>
                    <p>{{text}}</p>
                    <div>
                        {{#with button_text}}
                        <p><button class="btn btn-dark">{{solid}}</button></p>

                        <button class="btn btn-outline-dark">{{outline}}</button>
                        {{/with}}
                    </div>
                    {{/with}}
                </div>
            </div>
            {{/with}}
        </div>
    </div>
    <br>
    <br>

    {{!-- BLOCK 4 --}}
    <div class="card">
        <div class="card-body">
            {{#with nested_display}}
            <div class="display-1">{{title}}</div>
            <div class="card">
                <div class="card-body">
                    {{#with children_display}}
                    <div class="display-4">{{title}}</div>
                    <p>{{text}}</p>
                    <button class="btn btn-outline-warning">{{button.text}}</button>
                    {{/with}}
                </div>
            </div>
            <br>
            <h6>{{header_text}}</h6>
            <button class="btn btn-info text-light">{{button.text}}</button>
            <br>
            {{/with}}
        </div>
    </div>
    <br>
    <br>
    {{!-- BLOCK 5 --}}
    <div class="card">
        <div class="card-body">
            {{#with paragraph}}
            <p class="text-uppercase">{{title}}</p>
            <p class="text-lowercase">{{text}}</p>
            {{#with button}}
            <a href={{url}} target="_blank" class="btn btn-link">
                {{text}}
            </a>
            {{/with}}
            <div class="card">
                <div class="card-body">
                    <ul>
                        {{#each list_name}}
                        <li>
                            <a href={{url}} target="_blank" class="btn btn-link">
                                {{text}}
                            </a>
                        </li>
                        {{/each}}

                    </ul>
                </div>
            </div>

            {{/with}}
        </div>
    </div>


</main>